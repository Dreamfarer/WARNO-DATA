private template Template_Attack_Corridor
[
    MinimumResourceForLaunch:int = 0,
    RatioMinimumPourAttaquer:float = 1.0,
]
is TMacroActionDescriptor_CorridorAttack_Specific
(
    ParametresDeMission =
    ~/Default_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    ~/AvoidFillerCorridors_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TMacroActionProductionSettingHolder(ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]
        PoolModelList = [
            Pool_Default_Attack_1,
            Pool_Default_Attack_2,
            Pool_Default_Attack_3,
        ]
        TaggedPoolModelList =
        [
            (["AttackInfantry"], [ Pool_Infantry_Attack_1, Pool_Infantry_Attack_2, Pool_Infantry_Attack_3 ]),
            (["AttackCombined"], [ Pool_Combined_Attack_1, Pool_Combined_Attack_2, Pool_Combined_Attack_3 ]),
            (["AttackTank"], [ Pool_Tank_Attack_1, Pool_Tank_Attack_2, Pool_Tank_Attack_3 ]),
        ]
        LaunchASAP_RatioMin = 0.01),
        TAttackRatioSettingHolder(RatioMinimumPourAttaquer = <RatioMinimumPourAttaquer> CompterLesUnitesEnZoneExtendedDansLaMenace = true UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true),
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
        TAntiStackingSettingHolder(),
        TCorridorPlacementSettingHolder(StayInCorridorIfp = true MargeDeRegroupementDepuisLaLigneDeFront = 1 ActiveComportementEnBoutDeCouloir = true),
    ]
)

private template Template_Attack_MBT
[
    MinimumResourceForLaunch:int = 0,
    IsSupportingOtherAttacks:bool = false,
]
is TMacroActionDescriptor_CorridorDynamicDefense_Specific
(
    ParametresDeMission =
    ~/Default_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    ~/AvoidFillerCorridors_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TDefensePositionSettingHolder(IgnoreDefensePositionSafety = true),
        TMacroActionProductionSettingHolder(TaggedPoolModelList = [(["AttackMBT"], [~/Pool_Attack_MBT])] ProdProperties = [TProdProperty_LaunchAsSoonAsPossible]LaunchASAP_RatioMin = 0.01),
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
    ]
    +
    (
        <IsSupportingOtherAttacks> == false ? []:
        [
            TCoordinationSettingHolder(CoordinationType = ESupportingStatus/IsSupport),
        ]
    )
)

    [
        TGenerationSettingHolder(GenerationType = TGenerationType_Maintain),
    ]
)
//-------------------------------------------------------------------------
// Attaque d'objectifs
//-------------------------------------------------------------------------
private template Template_Objective_Attack
[
    MinimumResourceForLaunch:int = 0,
]
is TMacroActionDescriptor_ObjectiveAttack_Specific
(
    ParametresDeMission =
    ~/Default_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TMacroActionProductionSettingHolder(ProdProperties = [TProdProperty_LaunchAsSoonAsPossible] PoolModelList = [Pool_Objective_Attack] LaunchASAP_RatioMin = 0.01 DisableReinforcement = true),
        TAttackRatioSettingHolder(RatioMinimumPourAttaquer = 1.0 CompterLesUnitesEnZoneExtendedDansLaMenace = true UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true),
        TAttackRatioSettingHolder(DifficultyList = [IADifficulty/TresFacile] RatioMinimumPourAttaquer = 100.0 CompterLesUnitesEnZoneExtendedDansLaMenace = true UtiliserMenaceLaPlusProcheAuLieuDeLaPlusFaible = true),
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
    ]
)
//-------------------------------------------------------------------------
export Attack_Objectives is Template_Objective_Attack
(
)
//-------------------------------------------------------------------------
export Attack_Objectives_Autogen is Template_Objective_Attack
(
    MinimumResourceForLaunch = ~/MinimumResourceForLaunch_Stage_1
)
//-------------------------------------------------------------------------
// COULOIR
//-------------------------------------------------------------------------
private template Template_Main_Attack
[
    MinimumResourceForLaunch:int = 0,
]
is TMacroActionDescriptor_CorridorAttack_Specific
(
    ParametresDeMission =
    ~/Default_Attack_Settings
    +
    ~/Blitzkrieg_Attack_Settings
    +
    ~/AvoidFillerCorridors_Attack_Settings
    +
    [
        TGenerationSettingHolder(GenerationType = TGenerationType_FixedSize NumberLaunchBasedOnGenerationType = 1),
        TCoordinationSettingHolder(CoordinationType = ESupportingStatus/NeedSupport),
        TMacroActionProductionSettingHolder(ProdProperties = [TProdProperty_LaunchAsSoonAsPossible] PoolModelList = [~/Pool_Main_Attack]
            TaggedPoolModelList = [(["AttackMain"], [~/Pool_Main_Attack]),] PrioritizeCorridorsForTaggedPoolModels = true LaunchASAP_RatioMin = 0.01),
        TAttackRatioSettingHolder(RatioMinimumPourAttaquer = 1.2 RatioPourAttaquerPerduAChaqueTick = 0.00084 CompterLesUnitesEnZoneExtendedDansLaMenace = true), // Perte de 0.1 de ratio toutes les 2 minutes
        TMacroActionLifetimeSettingHolder(MinimumResourceForLaunch = <MinimumResourceForLaunch>),
        TCorridorPlacementSettingHolder(StayInCorridorIfp = true MargeDeRegroupementDepuisLaLigneDeFront = 1 ActiveComportementEnBoutDeCouloir = true),
    ]
)

BasicTextStyle is TTextStyleRTTI
(
    ColorBottom = BlancPur
    ColorUp     = BlancPur
    ColorStroke = NoirPur
    Stroke = false
    FontSize = 1
    AllowSmallFont = true
)

template TemplateTFS_Flag
[
    TextureToken     : string
] is TTFSCommand_UISymbol
(
    TextureToken = <TextureToken>
    BBMin=[0, -0.85, 0]
    BBMax=[1.7, 0.15, 0]
    ShaderDescriptor = $/M3D/Shader/MaterialInterface2D_Blend
)

template TFSCommand_ChangeColor
[
    Color = [255, 255, 255, 255]
] is TTFSCommand_StyleChangeRTTI
(
    Style = TTextStyleRTTI
    (
        FontSize    = 1
        ColorBottom = <Color>
        ColorUp     = <Color>
        Stroke = false
        AllowSmallFont = true
    )
)

export ResourceFeedbackTextSize is 18

template TFSCommand_ChangeColorForResourceFeedback
[
    Color = [255, 255, 255, 255]
] is TTFSCommand_StyleChangeRTTI
(
    Style = TTextStyleRTTI
    (
        FontSize    = ResourceFeedbackTextSize
        ColorBottom = <Color>
        ColorUp     = <Color>
        Stroke = false
        AllowSmallFont = true
    )
)


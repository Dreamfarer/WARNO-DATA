                    AlignementToAnchor = [0.5, 0.0]
                )
                Transition0 = 0
                Transition1 = 0.5
                Transition2 = 0.5
                Transition3 = 1

                TransitionColor0 = "OffmapFeedback/Gradient0"
                TransitionColor1 = "OffmapFeedback/Gradient1"

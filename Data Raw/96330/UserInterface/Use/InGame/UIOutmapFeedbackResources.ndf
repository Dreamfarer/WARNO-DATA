    NomreDeMotifsInterieurs = 36
    CouleurInterieure = [  235,   235,   235, 30] // 10

    //---------------------------------------------------------------------------------------//
    //--------------------- REGLAGES OBSERVATEUR CROIX     ----------------------------------//
    //---------------------------------------------------------------------------------------//
    EpaisseurCroix = 50.0 // 400
    LongueurCroix = 2000.0 // 8000
    CouleurCroix = [  235,   235,   235, 30] // 10

    //---------------------------------------------------------------------------------------//
